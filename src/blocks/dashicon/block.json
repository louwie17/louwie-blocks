{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "louwie-blocks/dashicon",
    "version": "0.1.0",
    "title": "Dashicon",
    "category": "widgets",
    "icon": "smiley",
    "description": "Rendering of a dashicon.",
    "supports": {
        "html": false
    },
    "attributes": {
        "size": {
            "type": "number"
        },
        "arrowColor": {
            "type": "string"
        },
        "alignment": {
            "type": "string",
            "default": "none"
        }
    },
    "textdomain": "louwie-blocks",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}